<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fifin.sistem_pakar_tbc">

    <uses-permission android:name="android.permission.INTERNET" />

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/healthbook"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/healthbook"
        android:supportsRtl="true"
        android:theme="@style/noActionBar">
        <activity android:name=".Screen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DiagnosaActivity" />
        <activity android:name=".InfoActivity" />
        <activity android:name=".TentangActivity" />
        <activity android:name=".Main5Activity" />
        <activity android:name=".KesimpulanActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".FormUserActivity"></activity>
    </application>

</manifest>